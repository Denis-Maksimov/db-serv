# 1 - создание тестовой таблицы 
CREATE TABLE IF NOT EXISTS test(ID,NAME,VALUE); INSERT INTO test VALUES(1,2,4);

# 2 - вставка значений в тестовую таблицу
INSERT INTO test VALUES("%s","%s","%s");

# 3 - поиск 
SELECT NAME FROM test WHERE VALUE = 4 ORDER BY ID DESC LIMIT 1
